package org.kh.java;

public class Variable6 {
	
	public static void main(String[] args) {
		float f1 = 3.14f;
		double f2 = 3.14;
		int kor = 90, eng = 80, mat = 95;	//같은 데이터형일때는 한번에 선언가능
		float avg = (kor + eng + mat) /3f;
		System.out.printf("\nf1=%.3", f1);
		System.out.printf("\nf2=%.2", f2);
		System.out.printf("\n평균 : %.2f", avg);
	}
}
