package org.kh.java;

public class Variable2 {

	public static void main(String[] args) {
		byte b = 10;
		short s = 20;//변수 선언과 초기화 한것(초기화란 값을 최초로 넣어주는것)
		int i;//변수 선언 만 한것
		long l;
		
		i = 30;
		l = 40;
		System.out.println("b="+b);
		System.out.println("s="+s);
		System.out.println("i="+i);
		System.out.println("l="+l);
	}

}
